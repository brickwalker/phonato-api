{
  "name": "phonato-api",
  "version": "0.1.0",
  "description": "Flexible API that works with NATO phonetic alphabet",
  "main": "./src/main.ts",
  "devDependencies": {
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
